        <formula>IF( 
OR( 
System_Log__c = 0, 
System_Log__c &lt; 11 
), 
IMAGE(&quot;/img/samples/color_green.gif&quot;,&quot;0 to 10 push&quot;,15,15), 
IF( 
System_Log__c = 11, 
IMAGE(&quot;/img/samples/color_yellow.gif&quot;,&quot;11 pushes&quot;,15,15), 
IF( 
System_Log__c &gt;= 12, 
IMAGE(&quot;/img/samples/color_red.gif&quot;,&quot;12 or more pushes&quot;,15,15), 
IMAGE(&quot;/img/samples/color_red.gif&quot;,&quot;12 or more pushes&quot;,15,15) 
)))</formula>
