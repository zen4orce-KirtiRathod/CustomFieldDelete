    <fields>
        <fullName>Activity_Monitor__c</fullName>
        <externalId>false</externalId>
        <formula>IF
(ISNULL(LastActivityDate),
IMAGE(&quot;/img/samples/color_red.gif &quot;, &quot;NO ACTIVITIES&quot;,15,15),
IF( ACI_Days_Since_Last_Activity__c &lt;15,
IMAGE(&quot;/img/samples/color_green.gif&quot;,&quot;Less than 30&quot;,15,15) ,
IF( ACI_Days_Since_Last_Activity__c &lt; 30,
IMAGE(&quot;/img/samples/color_yellow.gif&quot;,&quot;30 to 45&quot;,15,15),
IF( ACI_Days_Since_Last_Activity__c &gt;29,
IMAGE(&quot;/img/samples/color_red.gif&quot;,&quot;45 to 60&quot;,15,15) ,
IMAGE(&quot;/img/samples/color_red.gif&quot;,&quot;More than 60 Days&quot;,15,15)))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Green: Activity &lt; 15 Days

Yellow: Activity &gt; 15 Days

Red: Activity &gt;45 Days</inlineHelpText>
        <label>Activity Monitor</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
