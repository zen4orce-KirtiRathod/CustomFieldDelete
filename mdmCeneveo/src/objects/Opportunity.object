    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>Best_Case</fullName>
        <active>true</active>
        <description>Validates that the probability of a Best Case opportunity is properly set to &lt; 81%. This is useful for data cleanliness and reporting purposes.</description>
        <errorConditionFormula>AND(ISPICKVAL(  ForecastCategoryName  , &quot;Best Case&quot;),  OR(Probability   &lt;  0.51,  Probability   &gt; 0.80))</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability for Best Case Forecast Category must be between 51% and 80%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_Validate</fullName>
        <active>true</active>
        <description>Opportunity can&apos;t be closed with future date</description>
        <errorConditionFormula>If(IsClosed,
  IF(CloseDate &gt; TODAY(),True,False),False)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Opportunity can&apos;t be close with future Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Opportunity_Validate_Amount</fullName>
        <active>true</active>
        <description>When Opportunity is going to be close and amount is more than 2.5 lakh than a notification</description>
        <errorConditionFormula>AND(
    AND(
	IsClosed,
	IsWon, 
	Amount &gt;= 250000
       ),
       NOT(
       Amount_Verified__c 
       )
  )</errorConditionFormula>
        <errorMessage>Please verify the amount of the opportunity prior to closing. Notifications will be emailed to all levels of the organization once the opportunity is closed. If the amount is correct, check “Amount Verified?&quot; box. Thank you.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Dropped</fullName>
        <active>true</active>
        <description>If Closed Dropped is Stage, then Prob is 0%</description>
        <errorConditionFormula>AND ( ISPICKVAL(    StageName  , &quot;Closed Dropped&quot;), Probability &lt;&gt; 0)</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability must be 0% for a Dropped Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost</fullName>
        <active>true</active>
        <description>If Closed Lost is Stage, then Prob is 0%</description>
        <errorConditionFormula>AND ( ISPICKVAL(    StageName  , &quot;Closed Lost&quot;), Probability &lt;&gt; 0)</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability must be 0% for a Lost Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won</fullName>
        <active>true</active>
        <description>Validates that the probability of a won opportunity is properly set to 100%. This is useful for data cleanliness and reporting purposes.</description>
        <errorConditionFormula>AND ( ISPICKVAL( StageName, &quot;Closed Won&quot;), Probability &lt;&gt; 1)</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability must be 100% for won opportunities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_Require_Year_1_Revenue</fullName>
        <active>true</active>
        <errorConditionFormula>AND( IsClosed =true, IsWon = true, ISNULL(Year_1_Estimated_Revenue__c))</errorConditionFormula>
        <errorDisplayField>Year_1_Estimated_Revenue__c</errorDisplayField>
        <errorMessage>You must enter the value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comic</fullName>
        <active>true</active>
        <description>Safeguard Comic Opportunities</description>
        <errorConditionFormula>CONTAINS(Product_Description__c, &quot;comic&quot; )</errorConditionFormula>
        <errorMessage>This opportunity must be approved by Jeff Statler, Mike Fragrassi or Gary Bohn before proceeding. Please contact your SFDC Administrator if there are any questions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Commit</fullName>
        <active>true</active>
        <description>Validates that the probability of a Commit opportunity is properly set to &gt; 80%. This is useful for data cleanliness and reporting purposes.</description>
        <errorConditionFormula>AND(ISPICKVAL(  ForecastCategoryName  , &quot;Commit&quot;),  OR(Probability   &lt;  0.81,  Probability   &gt; 0.99))</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability for Commit Forecast Category must be between 81% and 99%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__Check_CompanyId_Value</fullName>
        <active>true</active>
        <description>Throws an exception message if the company id contains non-numerics</description>
        <errorConditionFormula>AND(
	(LID__LinkedIn_Company_Id__c &lt;&gt; NULL),
	NOT(ISNUMBER(LID__LinkedIn_Company_Id__c))
)</errorConditionFormula>
        <errorDisplayField>LID__LinkedIn_Company_Id__c</errorDisplayField>
        <errorMessage>CompanyId should be a numeric value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Customer</fullName>
        <active>true</active>
        <description>Cannot select New Customer if sales in last 12 months</description>
        <errorConditionFormula>AND(ISPICKVAL( Type   , &quot;New Customer - New Business&quot;),  OR(  Account.Previous_2Last_12Mo_Sales__c    &gt; 0))</errorConditionFormula>
        <errorMessage>The account chosen has sales volume within the previous 12 months. Please correct the TYPE to be &quot;Existing Customer - New Business&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Customer_12_Months</fullName>
        <active>true</active>
        <description>Validate that New New is Used for Customer that are less than 12 months old.</description>
        <errorConditionFormula>AND(ISPICKVAL( Type   , &quot;Existing Customer - New Business&quot;),  OR(  Account.Previous_2Last_12Mo_Sales__c     &lt;=  0))</errorConditionFormula>
        <errorMessage>The account chosen does NOT have sales volume within the previous 12 months. Please correct the TYPE to be &quot;New Customer - New Business&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Omitted</fullName>
        <active>true</active>
        <description>If Omitted is Forecast Category, then Prob is 0%</description>
        <errorConditionFormula>AND ( ISPICKVAL(  ForecastCategoryName , &quot;Omitted&quot;), Probability &lt;&gt; 0)</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability must be 0% for Omitted in the Forecast Category or a Lost Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pipeline</fullName>
        <active>true</active>
        <description>Validates that the probability of a Pipeline opportunity is properly set to &lt; 50%. This is useful for data cleanliness and reporting purposes.</description>
        <errorConditionFormula>AND(ISPICKVAL(  ForecastCategoryName  , &quot;Pipeline&quot;),  OR(Probability   &lt;  0.01,  Probability   &gt; 0.50))</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability for Pipeline Forecast Category must be between 1% and 50%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFI_RFP_Validation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IsClosed,
     IsWon,
     RFI_RFP__c, 
     OR(ISNULL(Year_1_Estimated_GM__c) , 
	ISNULL( Year_1_Estimated_Revenue__c) ,
	ISNULL( Year_2_Estimated_GM__c) , 
	ISNULL( Year_2_Estimated_Revenue__c) , 
	ISNULL( Year_3_Estimated_GM__c) , 
	ISNULL( Year_3_Estimated_Revenue__c)
	) 
)</errorConditionFormula>
        <errorMessage>RFI/RFP opportunity require fileds :-  Year 1 Estimated GM %,Year 1 Estimated Revenue,Year 2 Estimated GM %,Year 2 Estimated Revenue,Year 3 Estimated GM %,Year 3 Estimated Revenue</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_ValidateCloseWon</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	NOT(HasOpportunityLineItem),
	IsClosed,
	IsWon
)</errorConditionFormula>
        <errorMessage>Closed won opportunity must have products associated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_To_Verified_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    AND(
	Amount_Verified__c ,
	OR(
	    NOT(IsClosed),
	    NOT(IsWon),
	    (Amount &lt; 250000)
	    )
	), 
    true,
    false
   )</errorConditionFormula>
        <errorDisplayField>Amount_Verified__c</errorDisplayField>
        <errorMessage>This field is only applicable when amount is greater than 25K.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Won_Lost_Margin</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR(ISPICKVAL(StageName, &quot;Closed Lost&quot;), 

ISPICKVAL(StageName, &quot;Closed Won&quot;) 
), 
ISBLANK( Estimated_Gross_Margin__c ) 
)</errorConditionFormula>
        <errorDisplayField>Incumbent__c</errorDisplayField>
        <errorMessage>The following need to be completed: Estimated Gross Margin %, Gross Margin Information, and Margin Comments. NOTE : IF YOU ARE CLOSING AN OPPORTUNITY THAT WAS QUOTED OUT OF LOTUS OR BILL EST AND IS GREATER THAN $50K, IT WILL NEED TO BE REQUOTED THRU HAGEN</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Won_Lost_Reason</fullName>
        <active>true</active>
        <description>If opportunity is Won or Lost, then the Won/Lost Reason field is required.</description>
        <errorConditionFormula>AND(
           OR(ISPICKVAL(StageName, &quot;Closed Lost&quot;), 
                 ISPICKVAL(StageName, &quot;Closed Dropped&quot;) ,
                  ISPICKVAL(StageName, &quot;Closed Won&quot;)
           ),
         ISBLANK(Won_Lost_Reason__c)
)</errorConditionFormula>
        <errorDisplayField>Won_Lost_Reason__c</errorDisplayField>
        <errorMessage>Please enter the reason why the Opportunity was won or lost.</errorMessage>
    </validationRules>
