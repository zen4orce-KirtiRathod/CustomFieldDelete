    <validationRules>
        <fullName>User_Leader_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR (AND(NOT(ISNULL(Sales_Leader_Ref__c )), NOT(ISBLANK(Sales_Leader_Ref__c )),Self_Sales_Leader__c) , AND( NOT(ISNULL(Regional_Sales_Leader_Ref__c)),NOT(ISBLANK(Regional_Sales_Leader_Ref__c)),Self_Regional_Sales_Leader__c ) , AND(NOT(ISNULL(Divisional_Sales_Leader_Ref__c )) ,NOT(ISBLANK(Divisional_Sales_Leader_Ref__c )) ,Self_Divisional_Sales_Leader__c ) )</errorConditionFormula>
        <errorMessage>You can&apos;t enter leader reference and checkboxes same time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_Manager_Change_Date_Required</fullName>
        <active>false</active>
        <description>User Manager Change Date Required when User Manager Change Date is updated.</description>
        <errorConditionFormula>AND( NOT(ISNEW()), ISCHANGED(ManagerId), ISNULL(User_Manager_Changed_Date__c))</errorConditionFormula>
        <errorMessage>Please enter a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_To_Required_Employee_Number</fullName>
        <active>true</active>
        <description>Validation rule to check whether employee number is not blank when creating new users</description>
        <errorConditionFormula>ISBLANK(EmployeeNumber)</errorConditionFormula>
        <errorMessage>Employee Number cannot be blank when adding a new user. Please contact HR for the valid Employee Number. If the user is not an Employee, contact Kathy Coates for more information.</errorMessage>
    </validationRules>
