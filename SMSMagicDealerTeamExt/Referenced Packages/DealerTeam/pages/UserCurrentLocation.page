<apex:page showHeader="false" sidebar="false">
    <style>
    body {
       background-color: #CFEEF8; 
    }
    
    #force .btn {
        display: inline-block;
        padding: 6px 12px;
        margin-bottom: 0;
        font-size: 14px;
        font-weight: normal;
        line-height: 1.428571429;
        text-align: center;
        white-space: nowrap;
        vertical-align: middle;
        cursor: pointer;
        background-image: none;
        border: 1px solid transparent;
        border-radius: 4px;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        -o-user-select: none;
        user-select: none;
    }
    #force .btn-default {
        color: #333333;
        background-color: #ffffff;
        border-color: #cccccc;
    }
    #force .btn-xs {
        padding: 4px 6px;
        font-size: 10px;
        line-height: 1.5;
        border-radius: 3px;
    }
    </style>
    <apex:form >
    <div id="force">
    <table cellpadding="0" cellspacing="0">
        <tr>
            <td width="45px;" style="color:#333333"><b>CO#</b></td>
            <td width="80px;" style="color:#333333"><b>Location</b></td>
            <td>&nbsp;</td>
        </tr>
        <tr>
            <td><apex:outputText value="{!$User.Division}" style="color:#333333" /></td>
            <td><apex:outputText value="{!$User.Dealership_Location__c}" style="color:#333333" /></td>
            <td><apex:commandButton value="Change" onClick="javascript:parent.window.location='/apex/SelectDealership';" style="color:#333333;" styleClass="btn btn-xs btn-default"/></td>
        </tr>       
    </table>
    </div>
    </apex:form>
</apex:page>