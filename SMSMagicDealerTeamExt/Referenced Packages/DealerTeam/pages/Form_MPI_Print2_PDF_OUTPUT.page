        <tr style="height:30px;" class="border_bottom wide_border">
          <td width="40%" colspan="2" >
          </td>
          <td width="20%" valign="bottom" >
            DATE:<apex:outputText rendered="true" value="{0, date, MMMM d','  yyyy}"><apex:param value="{!NOW()}" /></apex:outputText>
          </td>
          <td width="20%" valign="bottom" >
            <apex:outputText rendered="true">ORDER#:&nbsp;{!dealer__Inspection_Report__c.dealer__Repair_Order_Number__c}</apex:outputText>
          </td>
          <td width="20%" >
          </td>
        </tr>
        
        <tr style="height:30px;"  class="border_bottom">
          <td width="" colspan="2"  valign="bottom" >
            <apex:outputText rendered="true">NAME:&nbsp;{!dealer__Inspection_Report__c.dealer__Customer_Name__c}</apex:outputText>
          </td>
          <td width="40%" valign="bottom" colspan="2" >
