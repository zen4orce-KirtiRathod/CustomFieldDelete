<apex:page renderAs="pdf" applyHtmlTag="false" showHeader="false" showChat="false" standardStylesheets="false" standardController="dealer__Inspection_Report__c">
  <html>
    <head>
      <apex:stylesheet value="//fonts.googleapis.com/css?family=PT+Sans" />
      
      <style type="text/CSS">

        body {
          font-family: 'PT Sans', Dialog;
          font-size: 9px;
        }
        
        body table {
          border: none;
        }
        
        table {
          font-family: 'PT Sans', Dialog;
          /* border-spacing: 10px; */
          width: 100%;
        }

        .col {
          width: 48%;
          margin: 0 4% 0 0;
          float: left;
        }

        .col.last {
          margin-right: 0; 
        }
        
        .col > table {
          border: 1px solid #6D6E71;
          padding: 0;
          margin: 0 0 20px;
        }

        .col > table thead {
          margin: 0 0 5px 0;
        }

        .col > table th {
          background-color: #0094BF;
          padding: 7px 5px;
          font-size: 13px;
          color: #fff;
          font-weight: normal;
        }

        .col > table th.inverted {
          background-color: #ffffff;
          color: #0094BF;
        }

        .col > table td {
          padding: 5px 3px;
        }

        .col > table td.columned {
          padding: 0px;
        }

        .no_border {
          border: 0 !important;
        }

        td {
          font-family:  'PT Sans', Dialog;
        }

        th {
          font-family:  'PT Sans', Dialog;
        }

        .branded-text {
          color: #0094BF;
        }

        h1 {
          font-size: 23px;
          font-weight: normal;
        }

        h2 {
          font-size: 14px;
          font-weight: normal;
          margin: 0;
        }

        
        .wide_border td {
          border-bottom-width: 0;
        }
        
        #head_table {
          font-size: 10px;
        }

        .box {
          display: inline-block;
          margin-right: 0;
          height: 20px;
          width: 20px;
          border: 1px solid #6D6E71;
        }

        .box.note {
          width: 28px;
        }

        .box i {
          position: relative;
          border-width: 1px;
          border-style: solid;
          display: block;
          width: 18px;
          height: 18px;
          border-color: #fff;
          line-height: 18px;
          text-align: center;
        }

        .box.column {
          height: 25px;
          width: 23px;
          border: 0;
          display: block;
          line-height: 25px;
          float: left;
          opacity: .75;
        }

        .box.column i {
          line-height: 23px;
          height: 23px;
          width: 21px;
        }

        .ok i{ border-color: #A3CD3A; }
        .reccomended i{ border-color: #FEE616; }
        .immediate i { border-color: #EE3424; }
        .na i { border-color: #cccccc; }

        .ok.column i{ background-color: #A3CD3A; }
        .reccomended.column i{ background-color: #FEE616; }
        .immediate.column i { background-color: #EE3424; }
        .na.column i { background-color: #cccccc; }

        .legend .ok i{ background-color: #A3CD3A; }
        .legend .reccomended { background-color: #FEE616; }
        .legend .immediate { background-color: #EE3424; }

        ul.legend {
          list-style-type: none;
          margin: 10px 0;
          padding: 0;
          overflow: hidden;
        }

        ul.legend li{
          list-style-type: none;
          margin: 0 20px 0 0;
          padding: 0;
          display: block;
          float: left;
        }
        .footer {
          text-align: center;
          color: #0094BF;
          clear: both;
          display: block;
          width: 100%;
          margin: 0;
        }

        .col > table.collapsed-top {
          
        }

        .col > table.collapsed-lower {
          border-top: 0;
          margin-top: -20px; 
        }

        @page {
          /* Landscape orientation */
          size:portrait;
          margin: 0.25in;
        }

        .col > table td.columned.visible_text {
          line-height: 23px;
        }

        /***********
        PRINT STYLES
        ***********/
        
        .companyName, p.footer {
        	visibility: hidden;
        }

        .border_bottom > td {
          border-bottom: 1px solid transparent;
          color: #fff;
          visibility: hidden;
        }

        .col > table > tr > td[align="left"], .visible_text {
          color: #ffffff;
        }

        .col > table > tr > td[align="left"].hidden_text, .hidden_text {
          color: #000000;
        }
        */

        /*******
        OUTPUT CSS
        ********/
        
          
        .companyName, p.footer {
        	visibility: visible;
        }
          
        body {
          color: #ffffff;
        }
        .col > table {
            border: 1px solid transparent;
        }

        .col > table th {
            background-color: transparent; 
        }

        .col > table th.inverted {
          background-color:transparent ;
          color: #fff;
        }

        .border_bottom > td  {
          border-bottom: 1px solid #000;
          color: #000;
          visibility: visible;
        }

        .branded-text {
          color: #fff;
        }

        .box {
          border-color: transparent;
        }

        .box i { 
          border-color: transparent;
          color: #000;
        }

        .ok i{ border-color: transparent; }
        .reccomended i{ border-color: transparent; }
        .immediate i{ border-color: transparent; }
        .na i{ border-color: transparent; }
        .column.ok i{ background-color: transparent; }
        .column.reccomended i{ background-color: transparent; }
        .column.immediate i{ background-color: transparent; }
        .column.na i{ background-color: transparent; }
        .legend .ok i{ background-color: transparent; }
        .legend .reccomended { background-color: transparent; }
        .legend .immediate { background-color: transparent; }

        .footer {
          color: #fff
        }

        td > img {
          visibility: hidden;
        }

        .col > table > tr > td[align="left"], .visible_text {
          color: #000000;

        }

        .col > table td.columned.visible_text {
          line-height: 23px;
        }

        .col > table > tr > td[align="left"].hidden_text, .hidden_text {
          color: #ffffff;
        }
        
      </style>
    </head>
    <body>
      <table cellspacing="0" ><!-- DOCUMENT HEADER -->
        <tr>
          <td width="50%">
              <h2>
                  <apex:outputText value="{!$Organization.Name}" styleClass="companyName" />
              </h2>
            <!--apex:image url="{!$Resource.dealer__CrownLogo}" width="30%" height="40%" /-->
          </td>
          <td width="50%" align="right">
            <h1 class="branded-text">
              Multi-Point Vehicle Inspection
            </h1>
          </td>
        </tr>
      </table>
      
