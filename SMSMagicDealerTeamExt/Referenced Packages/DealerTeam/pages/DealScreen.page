<script type="text/javascript">
    var j$ = jQuery.noConflict();
    
        j$(document).ready(function() {

	        j$('input').keydown(function(event){
	            j$('#KeyLog').val(event.which);
	        });
	        
	        // Set Deal Date to Default
	        var currentTime = new Date();
	        var day = currentTime.getDate();
	        var month = currentTime.getMonth() + 1;
	        var year = currentTime.getFullYear();
	
	        if(j$('#dealScreen\\:dealForm\\:dealBody\\:NoDDLinkComponent\\:NoDDLink\\:dealDate').val() == '') {
	           j$('#dealScreen\\:dealForm\\:dealBody\\:NoDDLinkComponent\\:NoDDLink\\:dealDate').val(month + "/" + day + "/" + year);
	        }
	        
            
            /* Dock Tools */
            j$('#dockTools').Fisheye(
				{
					maxWidth: 50,
					items: 'a',
					itemsText: 'span',
					container: '.dockContainer',
					itemWidth: 40,
					proximity: 90,
					halign : 'center'
				}
			);  
        
	        /*  Subscreens Hooks */
	        j$('#dealScreen\\:dealForm\\:dealBody\\:tradeValue').keydown(function(event){
	            if(event.which == 39) {
	                j$('#trades').dialog({ 
	                        height : 450,
	                        width  : 850,
	                        modal  : true
	                    });
	            }
			});            

        
        ///////////////////////
        // Load application
        ///////////////////////
        
        j$('#commandInput').focus();
        var screenfocus = 'purchase';
        var editMode	= false;
          
        j$("input.inputBodyData, input.inputBodyInt, input.inputBodyText").keypress(function (evt) {
            var charCode = evt.charCode || evt.keyCode;
            if (charCode  == 13) {
                calcLoan();
                j$("#commandInput").focus();
                return false;
            }
        });
        
        
        
        j$("#commandInput").keypress(function(evt) {
               var commandIssued = j$(this).val();
               var charCode = evt.charCode || evt.keyCode;
               if(charCode == 13) {
                   (j$(this).val() == 1) ? j$('#dealScreen\\:dealForm\\:dealBody\\:dealNumber').focus() : false;
                   (j$(this).val() == 2) ? j$('#dealScreen\\:dealForm\\:dealBody\\:NoDDLinkComponent\\:NoDDLink\\:dealDate').focus() : false;
                   (j$(this).val() == 3) ? j$('#dealScreen\\:dealForm\\:dealBody\\:StockNumber').focus() : false;
                   (j$(this).val() == 4) ? j$('#dealScreen\\:dealForm\\:dealBody\\:Price').focus() : false;
                   (j$(this).val() == 5) ? j$('#dealScreen\\:dealForm\\:dealBody\\:AfterMarket').focus() : false;
                   (j$(this).val() == 6) ? j$('#dealScreen\\:dealForm\\:dealBody\\:Term').focus() : false;
                   (j$(this).val() == 7) ? j$('#dealScreen\\:dealForm\\:dealBody\\:Rate').focus() : false;
                   (j$(this).val() == 8) ? j$('#dealScreen\\:dealForm\\:dealBody\\:Days').focus() : false;
                   (j$(this).val() == 9) ? j$('#dealScreen\\:dealForm\\:dealBody\\:PPY').focus() : false;
                   (j$(this).val() == 10) ? j$('#dealScreen\\:dealForm\\:dealBody\\:PymntDate').focus() : false;
                   
                   (j$(this).val() == 11) ? j$('#dealScreen\\:dealForm\\:dealBody\\:tradeValue').focus() : false;
                   (j$(this).val() == 12) ? j$('#dealScreen\\:dealForm\\:dealBody\\:Payoff').focus() : false;
                   (j$(this).val() == 13) ? j$('#dealScreen\\:dealForm\\:dealBody\\:Deposit').focus() : false;
                   (j$(this).val() == 14) ? j$('#dealScreen\\:dealForm\\:dealBody\\:DownPayment').focus() : false;
                   (j$(this).val() == 15) ? j$('#dealScreen\\:dealForm\\:dealBody\\:Rebates').focus() : false;
                   (j$(this).val() == 16) ? j$('#dealScreen\\:dealForm\\:dealBody\\:DeferredDown').focus() : false;
                   (j$(this).val() == 17) ? j$('#dealScreen\\:dealForm\\:dealBody\\:State').focus() : false;
                   (j$(this).val() == 18) ? j$('#dealScreen\\:dealForm\\:dealBody\\:County').focus() : false;
                   (j$(this).val() == 19) ? j$('#dealScreen\\:dealForm\\:dealBody\\:CountyRate').focus() : false;
               
                   // Command Structure
                   if(commandIssued.substring(0,1) == 'P' || commandIssued.substring(0,1) == 'p') {
                   		screenfocus	=	'purchase';
                   		RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:dealTab','deal_TAB');
				   }
                   
                   if(commandIssued.substring(0,1) == 'T' && j$.isNumeric(commandIssued.substring(1, (commandIssued.length)))) {
                       j$('#dealScreen\\:dealForm\\:dealBody\\:Term').val(commandIssued.substring(1, (commandIssued.length)));
                   }
                   
                   if((commandIssued.substring(0,1) == 'T' || commandIssued.substring(0,1) == 't') && commandIssued.length==1) {
                   		screenfocus	=	'trades';
                   		RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:tradesTab','trades_Tab');
                   }
                   
                   if(commandIssued.substring(0,1) == 'V' || commandIssued.substring(0,1) == 'v') {
                       screenfocus = 'vehicle';
                       RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:vehicleTab','vehicle_TAB');
                   }
                   
                   if(commandIssued.substring(0,1) == 'C' || commandIssued.substring(0,1) == 'c') {
                       screenfocus = 'customer';
                       RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:customerTab','customer_TAB');
                   }
                   
				   if(commandIssued.substring(0,1) == 'W' || commandIssued.substring(0,1) == 'w') {
                       screenfocus = 'warranty';
                       RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:warrantyTab','warranty_Tab');
                   } 
                   
                   if(commandIssued.substring(0,1) == 'G' || commandIssued.substring(0,1) == 'g') {
                       screenfocus = 'gap';
                       RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:gapTab','gap_Tab');
                   }   
                   
                   if(commandIssued.substring(0,1) == 'A' || commandIssued.substring(0,1) == 'a') {
                   		screenfocus	=	'aftermarket';
                   		RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:afternarketTab','aftermarket_Tab');
                   }
                           
                   if(commandIssued.substring(0,1) == 'I' || commandIssued.substring(0,1) == 'i') {
                   		screenfocus	=	'insurance';
                   		RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:insuranceTab','insurance_Tab');
                   }
                   
                   if(commandIssued.substring(0,1) == 'F' || commandIssued.substring(0,1) == 'f') {
                   		if(screenfocus!='fees' && screenfocus!='forms') {
                   			screenfocus='fees';
                   			RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:feesTab','fees_Tab');
                   		}                   		
                   		if(screenfocus=='fees') {
                   			screenfocus='forms';
							RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:formsTab','forms_Tab');                   			
                   		} 
                   		if(screenfocus=='forms') {
                   			screenfocus='fees';                   			
                   			RichFaces.switchTab('dealScreen:dealForm:dealBody:dealTabPanel','dealScreen:dealForm:dealBody:feesTab','fees_Tab');
                   		}
                   }	
                   	
                   j$(this).val('');
              
               }  
        });
          
        j$('input.inputBodyData, input.inputBodyInt, input.inputBodyText').focus(function() {
              j$(this).css('border', '1px solid #757575');
              j$(this).select();
        });
          
        j$('input.inputBodyData').blur(function() {
              j$(this).css('border', '0px');
              if(j$.isNumeric(j$(this).val())) {
                  j$(this).parseNumber({format:"####.00", locale:"us"});
                  j$(this).formatNumber({format:"####.00", locale:"us"});
              }
        });
        
        j$('input.inputBodyInt').blur(function() {
              j$(this).css('border', '0px');
              if(j$.isNumeric(j$(this).val())) {
                  j$(this).parseNumber({format:"####", locale:"us"});
                  j$(this).formatNumber({format:"####", locale:"us"});
              }
        });
        
        j$('input.inputBodyText').blur(function() {
              j$(this).css('border', '0px');
        });
        
        // Format all strings based on load data
        j$('input.inputBodyInt').each(function() {
             j$(this).parseNumber({format:"####", locale:"us"});
             j$(this).formatNumber({format:"####", locale:"us"});
        });
        
        j$('input.inputBodyData').each(function() {
            if(j$.isNumeric(j$(this).val())) {
              j$(this).parseNumber({format:"####.00", locale:"us"});
              j$(this).formatNumber({format:"####.00", locale:"us"});   
            }
        });            
        
        /* Remote Data Actions */
        j$('#dealScreen\\:dealForm\\:dealBody\\:StockNumber').blur(function() {
            
            if(j$('#dealScreen\\:dealForm\\:dealBody\\:StockNumber').val().length > 3) {
            dealer.Deal_EXT.selectStockNumber(j$(this).val(), function(result, event) {
                if(event.status) {
                	cLog(result);
                	// Inventory Exists?
                	if(result.length==0) {
                		j$('#StockNumberDays').empty();
                		j$('<img/>', { 'src':'/s.gif', 'class':'unresolvedIcon', 'alt':'No inventory item found for this stock number.' }).appendTo('#StockNumberDays');
                		return false;
                	}
                	
                    // Begin loading vehicle into deal screen(s) procedure
                    j$('#StockNumberDays').text('('+result[0].dealer__Days_in_Stock__c+')');
                    j$('#dealScreen\\:dealForm\\:dealBody\\:Price').val(result[0].dealer__Coded_Cost__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:MSRP').val(result[0].dealer__MSRP__c);
                    j$('#labelYear').text(result[0].dealer__Year__c);
                    j$('#labelMake').text(result[0].dealer__Make__c);
                    j$('#labelModel').text(result[0].dealer__Model__c);
                    j$('#labelVIN').text(result[0].dealer__VIN__c);
                    // Deal Screen Numbers
                    j$('#dealScreen\\:dealForm\\:dealBody\\:Price').val();
                    
                    // Vehicle Screen
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleNUO').val(result[0].dealer__NUO__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleStockNumber').val(result[0].dealer__Stock_Number__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleList').val(result[0].dealer__MSRP__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleYear').val(result[0].dealer__Year__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleSalePrice').val(j$('#dealScreen\\:dealForm\\:dealBody\\:Price').val());
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleMake').val(result[0].dealer__Make__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleInvoicePrice').val(result[0].dealer__Invoice_Price__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleModel').val(result[0].dealer__Model__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleHoldback').val(result[0].dealer__HoldBack__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleTrim').val(result[0].dealer__Trim_Level__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehiclePack').val(result[0].dealer__Pack__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleExtColor').val(result[0].dealer__Exterior_Color__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleBodyType').val(result[0].dealer__Body_Type__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleOdometer').val(result[0].dealer__Mileage__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleLicenceNo').val(result[0].dealer__License_Number__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleBicExpDate').val(result[0].dealer__License_Exp_Date__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleMemo1').val(result[0].dealer__Memo_One__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleTitleType').val(result[0].dealer__Title_Type__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleMemo2').val(result[0].dealer__Memo_Two__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleFuelType').val(result[0].dealer__Fuel_Type__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleKeyCode1').val(result[0].dealer__Ignition_Key_Code__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleTotalTires').val(result[0].dealer__New_Tires__c);
                    j$('#dealScreen\\:dealForm\\:dealBody\\:VehicleKeyCode2').val(result[0].dealer__Ignition_Key_Code__c);
                    
                    
                    
                } else if (event.type === 'exception') {    
                         document.getElementById("ErrorMesssage").innerHTML = event.message;
                    } else {
                       document.getElementById("ErrorMessage").innerHTML = event.message;
                    }
                }, {escape:true});
           }
        });
    
    });
    
    function calcLoan() {
          Price                  =    j$('#dealScreen\\:dealForm\\:dealBody\\:Price').val();
          DownPayment            =    j$('#dealScreen\\:dealForm\\:dealBody\\:DownPayment').val();
          AnnualInterestRate     =    j$('#dealScreen\\:dealForm\\:dealBody\\:Rate').val() / 100;
          MonthlyInterestRate    =    AnnualInterestRate / 12;
          Term                   =    j$('#dealScreen\\:dealForm\\:dealBody\\:Term').val();
          Principal              =    Price - DownPayment;
          Days                   =    j$('#dealScreen\\:dealForm\\:dealBody\\:Days').val();
          
            var P = parseFloat(Principal);
            var r = parseFloat(MonthlyInterestRate);
            var N = parseFloat(Term);
            var A = P*(r*Math.pow(1+r,N))/(Math.pow(1+r,N)-1);
            var D = parseFloat(Days);
            var i = AnnualInterestRate / 12;
            
            var an = 0;
            var add= 0;
            if(D > 30) {
                an = (1 - 1 / Math.pow((1 + i), N)) / i;
                an = an.toFixed(9);
                add = an * (1 + i);
                add = add.toFixed(9);
                A = P * (1 + D / 30 * i) / add;
                
            }
          // alert(i + ' ' +an+ ' ' + add + ' ' + A);
          j$("#pmt").text(A.toFixed(2));
    }
    
    function tabFocus() {
    	j$('#commandInput').focus();
    }
    
    function seek(el) {
    	if(j$(el).length>0 && editMode==true) {
    		j$(el).focus();
    	} else {
    		j$('#commandInput').focus();
    	}
    }
    
	function cLog(msg) {
	    if(typeof window.console == 'object') {
	        console.log(msg);                  
	    }
	}
</script>

