temp=(historyCheckArray[i].Id);
temp=temp.slice(0,15);
for(var j=0;j&lt;records.length; j++){
if(records[j]==temp)
{
bool=true;
}
}  
}

var totNoRecords = records.length;
var recordStr = &apos;&apos; ;



 if( records[0] == null ) {
