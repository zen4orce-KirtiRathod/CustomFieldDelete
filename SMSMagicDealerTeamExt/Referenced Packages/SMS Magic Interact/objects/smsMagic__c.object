    } 
    else if( avlCredit &gt;= totNoRecords &amp;&amp; avlCredit &lt; 4* totNoRecords ) {

       var alertMsg = &apos;SMS would be sent to &apos; + totNoRecords + recordStr;
       alertMsg += &apos; Long messages would take more than 1 credit.&apos;;  
       if(confirm(alertMsg))
         resendSMS();       
    }
    else {
       if(confirm(&apos;SMS would be sent to &apos; + totNoRecords + recordStr )) {
          resendSMS();
       }
    }  
}

else{
